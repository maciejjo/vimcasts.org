{
    "config.rb": {
        "command": "config"
    },
    "source/*": {
        "command": "index"
    },
    "source/index.html.erb": {
        "command": "index"
    },
    "source/_*": {
        "command": "partial"
    },
    "source/blog/*": {
        "command": "blog"
    },
    "source/episodes/*.md": {
        "command": "episode",
        "alternate": "source/transcripts/%s/en.md"
    },
    "source/transcripts/*/en.md": {
        "command": "transcript",
        "alternate": "source/episodes/%s*.md"
    },
    "source/feeds/*": {
        "command": "feed"
    },
    "source/javascripts/*": {
        "command": "javascript"
    },
    "source/layouts/*": {
        "command": "layout"
    },
    "source/layouts/layout.erb": {
        "command": "layout"
    },
    "source/paths/*": {
        "command": "path"
    },
    "source/publications/*": {
        "command": "publication"
    },
    "source/stylesheets/*": {
        "command": "stylesheet"
    },
    "source/training/*": {
        "command": "training"
    }
}
